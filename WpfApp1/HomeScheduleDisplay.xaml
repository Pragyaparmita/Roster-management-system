<UserControl x:Class="WpfApp1.HomeScheduleDisplay"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfApp1"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>

        <Button Height="70" Width="550" Name="scheduleButton" VerticalAlignment="Center" >
            <Button.Content>
                <StackPanel>
                    <TextBlock Name="homeTxbEmployee" FontWeight="Bold" HorizontalAlignment="Center"></TextBlock>
                    <TextBlock Name="homeTxbTime" HorizontalAlignment="Center"></TextBlock>
                </StackPanel>
            </Button.Content>
        </Button>


    </StackPanel>
</UserControl>
